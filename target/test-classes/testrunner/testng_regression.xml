<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Softwaretestingboard App Test Regression Suite"  verbose="4">
<!-- thread-count="" 	parallel="tests" -->

	 
<listeners>
<listener
	class-name="com.qa.softwaretestingboard.listeners.ExtentReportListener" />
</listeners>



	<test name="Login Page Test" >
		<classes>
			<class name="com.qa.softwaretestingboard.tests.LoginPageTest" />	
			
				
		</classes>
	</test>


	
	 <test name="Register Page Test - chrome"> 
		   <parameter name="browser" value="chrome"></parameter> 
		<classes>
			<class name="com.qa.softwaretestingboard.tests.RegisterPageTest" />
		</classes>
	</test>

 <test name="Register Page Test - firefox"> 
		   <parameter name="browser" value="firefox"></parameter> 
		<classes>
			<class name="com.qa.softwaretestingboard.tests.RegisterPageTest" />
		</classes>
	</test>

	
</suite>	
